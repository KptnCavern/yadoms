{
	"type": "colorRGB",
	"version": "1.0.0-rc.1",
	"author": "Jean-Michel BERHAULT",
	"url": "https://github.com/Yadoms/yadoms/wiki/colorRGB",
	"credits": "",
  "dimensions": {
    "min": {
      "x": 2,
      "y": 2
    },
    "max": {
      "x": 4,
      "y": 4
    },
    "default": {
      "x": 2,
      "y": 2
    }
  },
  "hasTitle" :  true,
	"configurationSchema": {
		"device": {
			"type": "keyword",
			"expectedCapacity": "colorrgb",
			"expectedKeywordAccess": "get"
		},
      "preselector": {
          "type": "section",
          "content": {
            "colorsSection": {
              "type": "list",
              "nbItemsMin": "1",
              "nbItemsMax": "5",
              "allowDuplication": "true",
              "item": {
                "type": "section",
                "content": {
                  "color": {
                    "type": "color",
                    "defaultValue": "#FF0000"
                  }
                }
              }
            }
          }
      }
	}
}