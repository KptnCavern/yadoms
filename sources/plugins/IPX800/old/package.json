{
   "type": "IPX800",
   "version": "1.0.0",
   "releaseType": "beta",
   "author": "Jean-Michel BERHAULT",
   "url": "https://github.com/Yadoms/yadoms/wiki/IPX800",
   "credits": "",
   "supportedPlatforms":"all",
   "configurationSchema": {
		"IPAddress": {
		  "type": "string",
		  "regex" : "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}$",
		  "required" : "true"
		},
		"Port": {
		  "type": "int",
		  "required" : "true",
		  "defaultValue": "80"
		},
	    "authentication": {
		   "type": "section",
		   "enableWithCheckBox": "true",
		   "enableWithCheckBoxDefaultValue": "true",
		   "content": {
		      "Password": {
			     "type": "string",
			     "encrypted": "true"
			}
		 }
	  },
	  "extensions": {
         "type": "list",
		 "nbItemsMin": "0",
		 "nbItemsMax": "20",
		 "allowDuplication": "true",
		 "item": {
		    "type":"section",
			"content":{
			   "equipment":{
			      "type": "enum",
			      "values": {
			         "X-8D": "",
					 "X-24D": "",					 
			         "X-THL": "",
			         "X400-CT": "",
			         "X8R": ""
				  }
			   }
			}
		  }
		}
   },
   "supportManuallyDeviceCreation": "false",
   "manuallyDeviceCreationConfigurationSchema": {}
}