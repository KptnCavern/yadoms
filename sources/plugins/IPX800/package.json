{
   "type": "IPX800",
   "version": "1.0.0",
   "releaseType": "beta",
   "author": "Jean-Michel BERHAULT",
   "url": "https://github.com/Yadoms/yadoms/wiki/IPX800",
   "credits": "",
   "supportedPlatforms":"all",
   "configurationSchema": {
		"IPAddress": {
		  "type": "string",
		  "regex" : "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}$",
		  "required" : "true"
		},
		"Port": {
		  "type": "int",
		  "required" : "true"
		},
		"Account": {
		  "type": "string",
		  "required" : "true"
		},
		"Password": {
		  "type": "string",
		  "required" : "true",
		  "encrypted": "true"
		}   		
   }
}